    "jwks-rsa": "^3.0.1",
    "moment": "^2.29.4",
    "mongoose": "^7.0.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "odata-v4-mongodb": "^0.1.12",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
